{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "#pwlanding",
      "control": "exists"
    },
    "logout": {
      "type": "action",
      "control": "action.link",
      "selector": "a",
      "selectContains": "Sign Out",
      "selectHidden": true
    },
    "menuGroup": {
      "type": "array",
      "selectorAll": "#navbar",
      "items": {
        "type": "object",
        "properties": {
          "menuOption": {
            "type": "string",
            "selector": "li",
            "control": "property.innerText"
          }
        }
      }
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
