{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "#pwlanding",
      "control": "exists"
    },
    "sideMenuGroups": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "sideMenuItems": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "itemName": {
                  "type": "string",
                  "control": "property.innerText"
                },
                "action": {
                  "type": "action",
                  "control": "action.link"
                }
              }
            }
          },
          "groupName": {
            "type": "string",
            "selector": "a.dropdown-toggle",
            "control": "property.innerText"
          }
        }
      },
      "selectorAll": "#navbar > ul",
      "frameSelector": [
        null
      ]
    },
    "logout": {
      "type": "action",
      "control": "action.link",
      "selector": "a",
      "selectContains": "Sign Out",
      "selectHidden": true
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
