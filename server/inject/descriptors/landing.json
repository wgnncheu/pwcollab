{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "#pwlanding",
      "control": "exists"
    },
    "logout": {
      "type": "action",
      "control": "action.link",
      "selector": "a",
      "selectContains": "Sign Out",
      "selectHidden": true
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
