{
  "type": "object",
  "properties": {
    "isLanding": {
      "type": "identifier",
      "selector": "#pwlanding",
      "control": "exists"
    },
    "sideMenuGroups": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "sideMenuItems": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "itemName": {
                  "type": "string"
                },
                "action": {
                  "type": "action"
                }
              }
            }
          },
          "groupName": {
            "type": "string"
          }
        }
      },
      "selectorAll": "li.class=[\"menu-item\"]",
      "frameSelector": [
        null
      ]
    },
    "logout": {
      "type": "action",
      "control": "action.link",
      "selector": "a",
      "selectContains": "Sign Out",
      "selectHidden": true
    }
  },
  "connect": [
    {
      "match": [
        "isLanding"
      ],
      "screens": [
        "landing"
      ]
    }
  ]
}
