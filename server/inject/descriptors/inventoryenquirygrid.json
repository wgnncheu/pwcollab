{
  "type": "object",
  "comment": "TableView",
  "properties": {
    "listItems": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "description": {
            "type": "string",
            "label": "description"
          },
          "date": {
            "type": "string",
            "label": "Date - Time"
          },
          "action": {
            "type": "action"
          },
          "prtnum": {
            "type": "string"
          },
          "avqty": {
            "type": "string"
          },
          "supnam": {
            "type": "string",
            "label": "ListItemName"
          },
          "hldqty": {
            "type": "string"
          },
          "isInventoryEnquiryGrid": {
            "type": "identifier",
            "selector": "h2",
            "control": "exists",
            "selectContains": "Inventory Enquiry"
          }
        }
      },
      "label": "ListItems",
      "selectorAll": "#widgetListAtlasParts > div > div.k-grid-content.k-auto-scrollable > table > tbody > tr"
    },
    "random": {
      "type": "string",
      "selector": "h2",
      "control": "property.innerText"
    }
  },
  "connect": [
    {
      "match": [
        "listItems.isInventoryEnquiryGrid"
      ],
      "screens": [
        "inventoryenquirygrid"
      ]
    }
  ]
}
